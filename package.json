    "dependencies": {
        "body-parser": "^1.20.2",
        "cloudinary": "^1.41.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "mysql": "^2.18.1",
