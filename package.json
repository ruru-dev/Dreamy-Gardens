    "version": "0.0.1",
    "description": "Garden inspiration app",
    "main": "server.js",
    "scripts": {
        "build": "echo building...",
        "start": "node ./server.js",
        "dev": "nodemon ./server.js",
        "test": "echo \"Error: no test specified\" && exit 1",
        "format": "prettier --check \"./**/*.{js,jsx,css,json,html,md}\"",
        "format:fix": "prettier --write  \"./**/*.{js,jsx,css,json,html,md}\"",
        "fe:build": "BUILD_PATH='../public' npm run --prefix ./frontend build",
